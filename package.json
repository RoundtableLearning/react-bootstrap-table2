    "sass": "^1.58.3",
    "underscore": "1.9.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "packages/*/src/**/*.js",
      "packages/*/index.js",
      "!packages/react-bootstrap-table2-example/**/*.js"
    ],
    "roots": [
      "<rootDir>/packages"
    ],
    "setupFiles": [
      "<rootDir>/enzyme-setup.js"
    ],
    "modulePaths": [
      "<rootDir>/packages/react-bootstrap-table2"
    ],
    "testEnvironment": "node",
    "testMatch": [
